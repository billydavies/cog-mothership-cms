<ul class="social">
	{% if not networks.twitter.name is empty %}
	<li class="twitter">
		<a href="https://twitter.com/">Twitter</a>
	</li>
	{% endif %}
	{% if not networks.instagram.name is empty %}
	<li class-"instagram">
		<a href="http://instagram.com/">Instagram</a>
	</li>
	{% endif %}
	{% if not networks.facebook.name is empty %}
	<li class="facebook">
		<a href="https://www.facebook.com/">Facebook</a>
	</li>
	{% endif %}
	{% if not networks.pintrest.name is empty %}
	<li class="pinterest">
		<a href="http://www.pinterest.com/">Pinterest</a>
	</li>
	{% endif %}
</ul>