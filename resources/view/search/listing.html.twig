<h1>Search results for '{{ termsString }}'</h1>

{% for page in pages %}
	<h2><a href="{{ url('ms.cms.frontend', {slug: page.slug}) }}">{{ page.title }}</a></h2>
	<dl>
		<dt>Score</dt>
		<dd>{{ page.score }}</dd>
	</dl>
{% endfor %}

<nav>
	{% for i in 1..pagination.numPages %}
		<a href="#">{{ i }}</a>
	{% endfor %}
</nav>